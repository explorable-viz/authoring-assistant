let getByYear year data =
   let helper [] acc = acc;
      helper (x: xs) acc =
            if x.year == year
            then helper xs (x : acc)
            else helper xs acc
   in helper data [];
let sumEmissions year data =
   let records = getByYear year data;
   let totals = sum(map (fun x -> x.emissions) records)
   in totals

