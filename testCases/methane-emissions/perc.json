{
  "variables": {
    "emissions": [
      {
        "article": "",
        "type":"Agriculture",
        "year": "2030"
      },
      {
        "article": "The",
        "type": "Energy Sector",
        "year": "2030"
      }
    ]
  },
  "testing-variables": {},
  "datasets": [
    {
      "var": "tableData",
      "file": "datasets/methane-emissions.fld"
    }
  ],
  "imports": ["methane"],
  "paragraph": [
    {
      "type": "literal",
      "value": "$emissions.article$ $emissions.type$ accounts for total methane emissions of "
    },
    {
      "type": "expression",
      "expression": "numToStr ((record.emissions / sum(map (fun x -> x.emissions) (getByYear year tableData))) * 100)",
      "categories": ["data_retrieval"]
    },
    {
      "type": "literal",
      "value": "% in $emissions.year$."
    }
  ]
}

